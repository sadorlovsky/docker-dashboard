  },
  "homepage": "https://github.com/sadorlovsky/docker-dashboard",
  "dependencies": {
    "apollo-client": "^0.3.12",
    "express": "^4.13.4",
    "express-graphql": "^0.5.3",
    "got": "^6.3.0",
    "graphql": "^0.6.0",
    "graphql-tools": "^0.4.2",
    "isomorphic-fetch": "^2.2.1",
    "react": "^15.1.0",
    "react-apollo": "^0.3.5",
    "react-css-modules": "^3.7.6",
